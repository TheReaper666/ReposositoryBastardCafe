<UserControl x:Class="Eksamen2016.UCMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Eksamen2016"
             mc:Ignorable="d" 
             d:DesignHeight="270" d:DesignWidth="148">
    <Grid>
        <Image x:Name="image" 
               HorizontalAlignment="Left" 
               Height="127" 
               Margin="10,10,0,0" 
               VerticalAlignment="Top" 
               Width="128" 
               Source="Images/Ikon.png"/>
        <Button x:Name="buttonReservation" Content="Reservation" HorizontalAlignment="Left" Height="23" Margin="10,153,0,0" VerticalAlignment="Top" Width="128" Click="buttonReservation_Click"/>
        <Button x:Name="buttonCostumer" Content="Kunde" HorizontalAlignment="Left" Height="23" Margin="10,195,0,0" VerticalAlignment="Top" Width="128"/>
        <Button x:Name="buttonGame" Content="Spil" HorizontalAlignment="Left" Height="23" Margin="10,236,0,0" VerticalAlignment="Top" Width="128"/>

    </Grid>
</UserControl>
