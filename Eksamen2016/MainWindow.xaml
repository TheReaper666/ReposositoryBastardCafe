<Window x:Class="Eksamen2016.MainWindow"
        x:Name="MainForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Brætspilklubben BASTARD CAFE" 
        Height="310.725" 
        Width="750" 
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        WindowStyle="SingleBorderWindow"
        Icon="Images/Ikon.png" Closed="MainForm_Closed">

    <Grid Margin="3,3,3,3">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="148*" />
            <ColumnDefinition Width="223*" />
            <ColumnDefinition Width="373*" />
        </Grid.ColumnDefinitions>
        <UserControl x:Name="UCLeft" Grid.Column="0" />
        <UserControl x:Name="UCCenter" Grid.Column="1" />
        <UserControl x:Name="UCRight" Grid.Column="2" />
    </Grid>
</Window>
